{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "classnames": "^2.3.1",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.17",
    "umi-request": "^1.3.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.1",
    "@umijs/fabric": "^2.6.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.17",
    "eslint-plugin-prettier": "^3.4.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
